{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "functionTimeout": "00:10:00",
  "extensions": {
    "timers": {
      "timeZone": "America/New_York" // Set to UTC for GMT
    }
  }
}